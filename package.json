{
  "name": "cree-intelligent-dictionary",
  "version": "1.0.0",
  "description": "Cree Intelligent Dictionary",
  "scripts": {
    "start": "webpack --watch & pipenv run dev",
    "bg-watch": "webpack --watch &",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss -o CreeDictionary/CreeDictionary/static/CreeDictionary/css/styles.css src/css/styles.css ",
    "build:js": "webpack",
    "test": "USE_TEST_DB=true pipenv run dev & $(npm bin)/wait-on tcp:127.0.0.1:8000 && $(npm bin)/cypress run",
    "cypress": "$(npm bin)/wait-on tcp:127.0.0.1:8000 && $(npm bin)/cypress run",
    "test:ci": "USE_TEST_DB=true pipenv run dev & $(npm bin)/wait-on tcp:127.0.0.1:8000 && $(npm bin)/cypress run --record"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UAlbertaALTLab/cree-intelligent-dictionary.git"
  },
  "author": "University of Alberta ALTLab",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/UAlbertaALTLab/cree-intelligent-dictionary/issues"
  },
  "homepage": "https://github.com/UAlbertaALTLab/cree-intelligent-dictionary#readme",
  "devDependencies": {
    "cssnano": "^4.1.10",
    "cypress": "^3.6.0",
    "eslint": "^6.5.1",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.3",
    "postcss-custom-properties": "^9.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "rollup": "^1.27.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "jquery": "^3.4.1"
  }
}
